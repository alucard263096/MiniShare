<!--pages/map/map.wxml-->
<view class='superheader'></view>
<text class='fa fa-chevron-left btnback' bindtap='back'></text>
<map id="map" longitude="{{lng}}" latitude="{{lat}}" scale="14" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" show-location style="width: 100vw; height: 100vh;"></map>
